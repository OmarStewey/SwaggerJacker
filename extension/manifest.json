{
  "manifest_version": 2,
  "name": "Swagger Jacker",
  "version": "1",
  "description": "Find swag, Tag swag.",

  "browser_action": {
    "icons": [ "icon.png" ],
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "models.js", "SwaggerJacker.js", "background.js"],
      "all_frames":true
    }],
  "permissions": [
    "*://*/*"
  ]
}